<div class="tweet container">
	<div class="jumbotron">
	<h1>Tweet#Edit</h1>
		<div class="form-group">
		<%= form_for (@tweet) do |f| %>
			<b>Title<%= f.text_field :title, class: 'form-control' %>
			</b>
			<b>Content<br>
			<%= f.text_area :content, class: 'form-control test_class'%>
			</b>
			<b>Image<br>
				<%= attachment_image_tag @tweet,:image, :fill, 60,60, class: "profile-thumb",fallback: "twitter.png",:size => "30Ã—30" %><%= f.attachment_field :image %>
			</b>
			<p><%= f.submit 'Update Tweet', class: 'btn btn-primary btn-block' %></p>
		<% end %>
		</div>
	</div>
</div>
