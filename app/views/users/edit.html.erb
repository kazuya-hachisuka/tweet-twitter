<div class="tweet container">
	<div class="jumbotron col-md-12">
		<div class="form-group">
		<%= form_for(@user) do |f| %>
			<label>Name</label><br>
			<%= f.text_field :name, autofocus: true, autocomplete: "Name", class:'imput' %><br>
			<label>Image</label><br>
			<%= f.attachment_field :profile_image %><br>
			<%= f.submit "Update user", class: "btn btn-warning btn-xs"%>
		<% end %>
	</div>
	</div>
</div>